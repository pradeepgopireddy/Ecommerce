<% if @review.valid? %>
	$('#reviewNotice').html("Thanks for your review");
	$('#reviewList').append("<%= escape_javascript render 'review' %>");
	$('#new_review')[0].reset();
	$('#reviewError').hide();
	setTimeout(function(){
		$('#reviewNotice').slideUp();
	},5000);
	// swal("Good job!", "Review added Successfully!", "success");
<% else %>
	$('#reviewError').html("<%= escape_javascript render 'error' %>");
<% end %>